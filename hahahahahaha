def findandsensor(word,start,end):
    censored=""
    counter=0
    for letter in word:
        if counter >= start and counter <= end:
            censored += "*"
        else:
            censored += letter
        counter += 1
    return censored
listswear= ["bobo","tanga","gago","ulol","tangina","dick","suck","fuck","hayop","pussy"]

intText = input('Type here: ')
listw= intText.split()
stat= False
for word in listw:
    for swear in listswear:
        if word.find(swear) > -1:
            start = word.find(swear)
            end = start + (len(swear) -1)
            stat = True
            break
        else:
            stat = False
    if not stat:
            print(word,end= " ")
    else:
            print(findandsensor(word,start,end),end= " ")
            

