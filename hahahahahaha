string_methods_red_rubio.py


print("#1Capitalized")
word = "hello"
print(word.capitalize())
print()
print("#Islower")
word = "hi"
print(word.islower())
print()
print("#Center")
Joel = "Joel"
print("Hello: ",Joel.center(20))
print()
print("#Isnumeric")
number = "12345"
print(number.isnumeric())
print()
print("#Casefold")
word = "HELLO WORLD"
print(word.casefold())
print()
print("#Isalpha")
word = "hello world"
print(word.isalpha())


